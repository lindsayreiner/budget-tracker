{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "npm run build && node server.js",
    "start:dev": "concurrently \"npm run server\" \"cd client && npm run start\"",
    "server": "nodemon server.js --ignore client",
    "build": "cd client && npm run build",
    "install": "cd client && npm install",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lindsayreiner/budget-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lindsayreiner/budget-tracker/issues"
  },
  "homepage": "https://github.com/lindsayreiner/budget-tracker#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lite-server": "^2.6.1",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {},
  "keywords": []
}
